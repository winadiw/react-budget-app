{
  "env": {
    "development": {
      "presets": ["@babel/react", "@babel/env"],
      "plugins": ["transform-es2015-modules-commonjs", "transform-class-properties"]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "@babel/plugin-proposal-class-properties"
      ],
      "presets": ["@babel/preset-react"]
    }
  }
}
